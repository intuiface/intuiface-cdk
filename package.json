{
  "name": "@intuiface/intuiface-cdk",
  "version": "1.1.2",
  "license": "MIT",
  "workspaces": [
    "libs/*"
  ],
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "nx": "nx",
    "start": "ng serve",
    "build": "nx build core --prod && nx build components --prod && nx build interface-asset-schematics --prod && nx build custom-asset-schematics --prod",
    "lint": "npx eslint -c .eslintrc.js --ext .ts .",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "prepare": "npx shx cp -u ./.githooks/* ./.git/hooks/",
    "docs": "typedoc",
    "docs-all": "npm run docs --workspaces --if-present"
  },
  "dependencies": {
    "@angular-architects/module-federation": "^14.3.10",
    "@angular-devkit/core": "^14.2.5",
    "@angular-devkit/schematics": "^14.2.5",
    "@angular/animations": "14.2.5",
    "@angular/common": "14.2.5",
    "@angular/compiler": "14.2.5",
    "@angular/core": "14.2.5",
    "@angular/forms": "14.2.5",
    "@angular/platform-browser": "14.2.5",
    "@angular/platform-browser-dynamic": "14.2.5",
    "@angular/router": "14.2.5",
    "ng-packagr": "14.2.1",
    "rxjs": "^7.5.7",
    "tslib": "^2.4.1",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.2.5",
    "@angular-eslint/eslint-plugin": "^14.1.2",
    "@angular-eslint/eslint-plugin-template": "^14.1.2",
    "@angular-eslint/template-parser": "^14.1.2",
    "@angular/cli": "~14.2.0",
    "@angular/compiler-cli": "14.2.5",
    "@angular/language-service": "14.2.5",
    "@nrwl/angular": "14.8.4",
    "@nrwl/cli": "14.8.4",
    "@nrwl/eslint-plugin-nx": "14.8.4",
    "@nrwl/js": "14.8.4",
    "@nrwl/linter": "14.8.4",
    "@nrwl/node": "14.8.4",
    "@nrwl/nx-cloud": "14.7.0",
    "@nrwl/workspace": "14.8.4",
    "@types/estree": "^1.0.0",
    "@types/jest": "^27.4.0",
    "@types/json-schema": "^7.0.11",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/eslint-plugin-tslint": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "cypress": "^9.3.1",
    "dotenv": "^15.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "nx": "14.8.4",
    "prettier": "2.7.1",
    "shx": "^0.3.4",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "typedoc": "^0.24.8",
    "typedoc-plugin-markdown": "^3.15.4",
    "typescript": "4.8.4"
  }
