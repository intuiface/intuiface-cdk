{
    "convention": {
        "commitTypes": [
            "feat",
            "feature",
            "fix",
            "perf",
            "refactor",
            "style",
            "test",
            "build",
            "docs",
            "chore",
            "ci",
            "revert"
        ],
        "commitScopes": [
            "core",
            "components",
            "interface-asset",
            "custom-asset",
            "intuiface-cdk",
            "*"
        ],
        "releaseTagGlobPattern": "v[0-9]*.[0-9]*.[0-9]*",
        "issueRegexPattern": "[A-Z]{3,}-\\d+"
    }
}